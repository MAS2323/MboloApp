{
  "expo": {
    "name": "MboloApp",
    "slug": "MboloApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "scheme": ["mboloapp"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#4c86A8"
      },
      "package": "com.masthemas.MboloApp",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4c86A8"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fa723567-dbb5-4b3b-99fb-ff73f88bfde2"
      }
    },
    "owner": "masthemas",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fa723567-dbb5-4b3b-99fb-ff73f88bfde2"
    }
  }
}
